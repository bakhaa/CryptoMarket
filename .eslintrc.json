{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0],
    "object-curly-newline": [0],
    "import/prefer-default-export": [0],
    "no-confusing-arrow": 0,
    "react/no-array-index-key": 0,
    "comma-dangle": 0,
    "react/jsx-no-bind": 0
  }
}
